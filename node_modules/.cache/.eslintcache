rage.gamesWon = 0;\n    };\n\n    if (localStorage.gamesPlayed) {\n    } else {\n        localStorage.gamesPlayed = 0;\n    };\n\n    // increase gamesWon and gamesPlayed accordingly\n    if (ans_letters.size !== 0 && right.size >= ans_letters.size) {\n        localStorage.gamesPlayed = parseInt(localStorage.gamesPlayed) + 1;\n        localStorage.gamesWon = parseInt(localStorage.gamesWon) + 1;\n    } else if (mistake >= maxWrong) {\n        localStorage.gamesPlayed = parseInt(localStorage.gamesPlayed) + 1;\n    };\n\n    // if user guesses a letter correctly (clicks a button), letter is added into {right} prop\n    // else, letter is added into {wrong} prop\n    // {mistake} is the count of the number of wrong guesses the user has made. It will increase by 1 whenever the user makes a wrong guess.\n    // whoever is making the win/lose functionality can make use of the {mistake} prop\n\n    return (\n        <div className=\"App\">\n            <div style={{ backgroundColor: \"#90ee90\", padding: \"10px\" }}>\n                <h1>Hangman Game</h1>\n            </div>\n            <div className=\"gameBody\">\n            <br></br>\n            <ModalCategory></ModalCategory>\n            <img src={body[mistake]}></img>\n            <Blanks right={right}></Blanks>\n            <br></br>\n            <br></br>\n            {ans_letters.size !== 0 && right.size >= ans_letters.size ? (\n                <div>\n                    <h1>You Win! Good job üëèüèª</h1>\n                    <img src={winGif}></img>\n                    <h2>Games Played: {localStorage.gamesPlayed}</h2>\n                    <h2>Win Percentage: {localStorage.gamesPlayed !== '0' ? Math.round((localStorage.gamesWon)/localStorage.gamesPlayed*100) : 0}%</h2>\n                </div>\n            ) : mistake >= maxWrong ? (\n                <div>\n                    <h1>Aww, you can do better! üí™üèª</h1>\n                    <img src={lossGif}></img>\n                    <h2>Games Played: {localStorage.gamesPlayed}</h2>\n                    <h2>Win Percentage: {localStorage.gamesPlayed !== '0' ? Math.round((localStorage.gamesWon)/localStorage.gamesPlayed*100) : 0}%</h2>\n                </div>\n            ) : answer !== \"\" ? (\n                <Buttons\n                    right={right}\n                    updateRight={setRight}\n                    wrong={wrong}\n                    updateWrong={setWrong}\n                    mistake={mistake}\n                    updateMistake={setMistake}\n                />\n            ) : (\n                <p></p>\n            )}\n            {/* add reset */}\n            </div>\n        </div>\n    );\n}\n\nexport default App;\n","/Users/musyaffaq/Documents/GitHub/hangman-tiktok-project/hangman-game/src/reportWebVitals.js",[],[],"/Users/musyaffaq/Documents/GitHub/hangman-tiktok-project/hangman-game/src/components/Buttons.js",[],[],"/Users/musyaffaq/Documents/GitHub/hangman-tiktok-project/hangman-game/src/components/CategoriesInModal.js",[],[],[],"/Users/musyaffaq/Documents/GitHub/hangman-tiktok-project/hangman-game/src/components/Hangman.js",[],[],[],"/Users/musyaffaq/Documents/GitHub/hangman-tiktok-project/hangman-game/src/components/CategoriesBtn.js",[],[],{"ruleId":"43","severity":1,"message":"44","line":75,"column":13,"nodeType":"45","endLine":75,"endColumn":38},{"ruleId":"43","severity":1,"message":"44","line":82,"column":21,"nodeType":"45","endLine":82,"endColumn":39},{"ruleId":"43","severity":1,"message":"44","line":89,"column":21,"nodeType":"45","endLine":89,"endColumn":40},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement"]